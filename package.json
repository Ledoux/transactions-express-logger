{
  "ava": {
    "babel": {
      "presets": [
        "latest"
      ]
    },
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  },
  "author": "Erwan Ledoux",
  "dependencies": {
    "morgan": "^1.8.1",
    "winston": "^2.3.1"
  },
  "description": "An express framework for implementing an express logger",
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-cli": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "concurrently": "^3.1.0",
    "nodemon": "^1.11.0"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "name": "transactions-express-logger",
  "scripts": {
    "compile": "./node_modules/.bin/babel --presets latest -d lib/ src/",
    "test": "ava -v",
    "dev": "concurrently \"npm run dev-watch\"",
    "dev-watch": "nodemon --watch src --exec \"npm run compile\""
  },
  "version": "0.0.3"
}
